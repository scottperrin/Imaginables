	.hidden {
 		display: none;
	}

	/* Some things need aligning */
	.align-right {
		text-align: right;
	}

	/* Why the hell do lists still put their style outside the div? */
	ol, ul {
		list-style-position: inside;
  		margin-left:0;
	}
	
